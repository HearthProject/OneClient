<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <BorderPane layoutX="10.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <left>
                <VBox BorderPane.alignment="TOP_CENTER">
                    <children>
                        <Button fx:id="buttonBack" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Back To Instances" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                            </VBox.margin>
                        </Button>
                        <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                            </VBox.margin>
                            <children>
                                <VBox alignment="CENTER" HBox.hgrow="NEVER">
                                    <children>
                                        <Label fx:id="textPackName" style="-fx-font-size: 19px;" text="Modpack Name">
                                            <VBox.margin>
                                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                                            </VBox.margin>
                                        </Label>
                                        <Label fx:id="textMinecraftVersion" text="Minecraft Version: 0.0.0">
                                            <VBox.margin>
                                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                                            </VBox.margin>
                                        </Label>
                                        <ImageView fx:id="packIcon" fitHeight="130.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true"/>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                        <Button fx:id="buttonPlay" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Play">
                            <VBox.margin>
                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                            </VBox.margin>
                        </Button>
                        <Button fx:id="buttonOpenFolder" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Open Folder">
                            <VBox.margin>
                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                            </VBox.margin>
                        </Button>
                        <Button fx:id="buttonEditVersion" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Change Version">
                            <VBox.margin>
                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                            </VBox.margin>
                        </Button>
                        <Button fx:id="buttonExportPack" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Export Pack Zip">
                            <VBox.margin>
                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                            </VBox.margin>
                        </Button>
                        <Button fx:id="buttonGetCurseMods" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Download Mods">
                            <VBox.margin>
                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                            </VBox.margin>
                        </Button>
                        <Button fx:id="buttonDelete" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Delete Instance">
                            <VBox.margin>
                                <Insets bottom="5.0" left="20.0" right="20.0" top="5.0"/>
                            </VBox.margin>
                        </Button>
                    </children>
                    <BorderPane.margin>
                        <Insets/>
                    </BorderPane.margin>
                </VBox>
            </left>
            <center>
                <ListView fx:id="modList" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
            </center>
        </BorderPane>
    </children>
</AnchorPane>

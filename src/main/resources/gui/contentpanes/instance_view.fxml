<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <BorderPane layoutX="10.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <left>
                <VBox prefHeight="375.0" prefWidth="150.0" BorderPane.alignment="CENTER">
                    <children>
                        <GridPane>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <Text fx:id="textPackName" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="ModPack Name" GridPane.halignment="CENTER" GridPane.rowIndex="1">
                                    <font>
                                        <Font size="19.0"/>
                                    </font>
                                </Text>
                                <Text fx:id="textMinecraftVersion" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Minecraft: 0.0.0" GridPane.halignment="CENTER" GridPane.rowIndex="2"/>
                                <Button fx:id="buttonBack" mnemonicParsing="false" text="Back To Instances" GridPane.halignment="CENTER">
                                    <GridPane.margin>
                                        <Insets top="5.0"/>
                                    </GridPane.margin>
                                </Button>
                            </children>
                        </GridPane>
                        <ImageView fx:id="packIcon" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                            <VBox.margin>
                                <Insets bottom="5.0"/>
                            </VBox.margin>
                        </ImageView>
                        <VBox>
                            <children>
                                <Button fx:id="buttonPlay" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Play" VBox.vgrow="ALWAYS">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </Button>
                                <Button fx:id="buttonOpenFolder" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Open Folder" VBox.vgrow="ALWAYS">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </Button>
                                <Button fx:id="buttonEditVersion" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Change Version"
                                        VBox.vgrow="ALWAYS">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </Button>
                                <Button fx:id="buttonGetCurseMods" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Download Mods"
                                        VBox.vgrow="ALWAYS">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </Button>
                            </children>
                        </VBox>
                    </children>
                </VBox>
            </left>
            <center>
                <ListView fx:id="modList" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
            </center>
        </BorderPane>
    </children>
</AnchorPane>

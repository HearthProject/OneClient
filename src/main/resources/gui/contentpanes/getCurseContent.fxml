<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="0.0" minWidth="0.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox minHeight="-Infinity" minWidth="-Infinity" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox prefHeight="40.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="buttonInstall" mnemonicParsing="false" text="Install">
                            <HBox.margin>
                                <Insets left="5.0" top="5.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="buttonBrowse" mnemonicParsing="false" text="Browse Modpacks">
                            <HBox.margin>
                                <Insets left="15.0" top="5.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="buttonImport" mnemonicParsing="false" text="Import Modpack From Zip">
                            <HBox.margin>
                                <Insets left="15.0" top="5.0" />
                            </HBox.margin>
                        </Button>
                  <Button fx:id="buttonBack" mnemonicParsing="false" text="Back">
                     <HBox.margin>
                        <Insets left="15.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox fx:id="hBox" minWidth="200.0" prefHeight="513.0" prefWidth="1087.0" VBox.vgrow="ALWAYS">
                    <children>
                  <ScrollPane fx:id="scroll">
                     <content>
                              <VBox fx:id="vBox" prefHeight="513.0" prefWidth="534.0" />
                     </content>
                  </ScrollPane>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="5.0" />
    </padding>
</AnchorPane>

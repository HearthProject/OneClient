<fx:root maxHeight="192.0" maxWidth="192.0" prefHeight="192.0" prefWidth="192.0" type="javafx.scene.layout.StackPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox alignment="CENTER">
            <children>
                <JFXButton fx:id="displayButton" alignment="CENTER" contentDisplay="BOTTOM" focusTraversable="false" graphicTextGap="6.0" maxHeight="192.0" maxWidth="192.0" prefHeight="192.0" prefWidth="192.0" ripplerFill="#0d9179" style="-fx-border-width: 0; -fx-background-color: -oc-dark; -fx-font-family: 'Raleway' sans-serif;" text="Modpack Name" textAlignment="CENTER" VBox.vgrow="NEVER">

                              <ImageView fx:id="imageView" fitHeight="140.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true" />
                            <StackPane alignment="BOTTOM_CENTER" prefWidth="200.0">
                                <children>
                                    <Rectangle fill="#1b1b1b" height="19.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="92.0" />
                              <Text fx:id="statusText" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: -oc-text;" text="Status Text">

<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXToggleButton?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="anchorPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS"
            xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox fx:id="vbox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label id="oc-label" fx:id="title" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Installing to ...">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Label>
                <HBox VBox.vgrow="NEVER">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                    <children>
                        <Button fx:id="buttonBack" mnemonicParsing="false" text="Back">
                            <HBox.margin>
                                <Insets bottom="6.0" left="6.0" right="6.0" top="6.0"/>
                            </HBox.margin>
                        </Button>
                        <Label fx:id="page" minHeight="-Infinity" prefHeight="28.0" text="Label">
                            <HBox.margin>
                                <Insets bottom="6.0" left="6.0" right="6.0" top="6.0"/>
                            </HBox.margin>
                        </Label>
                        <ProgressIndicator fx:id="loadingIcon" minHeight="-Infinity" minWidth="-Infinity" prefHeight="32.0" prefWidth="32.0">
                            <HBox.margin>
                                <Insets bottom="6.0" left="6.0" right="6.0" top="6.0"/>
                            </HBox.margin>
                        </ProgressIndicator>
                        <TextField fx:id="textSearch" alignment="CENTER" prefHeight="28.0" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="6.0" left="6.0" right="6.0" top="6.0"/>
                            </HBox.margin>
                        </TextField>
                        <Button fx:id="buttonSearch" defaultButton="true" mnemonicParsing="false" prefHeight="28.0" text="Search">
                            <HBox.margin>
                                <Insets bottom="6.0" left="6.0" right="6.0" top="6.0"/>
                            </HBox.margin>
                        </Button>
                        <ComboBox fx:id="filterSort" prefHeight="28.0" prefWidth="175.0">
                            <HBox.margin>
                                <Insets bottom="6.0" left="6.0" right="6.0" top="6.0"/>
                            </HBox.margin>
                        </ComboBox>
                        <JFXToggleButton fx:id="toggleSort" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" text="Reverse" unToggleColor="#282828" unToggleLineColor="#4F4F4F">
                            <HBox.margin>
                                <Insets bottom="6.0" left="6.0" right="6.0" top="6.0"/>
                            </HBox.margin>
                        </JFXToggleButton>
                    </children>
                </HBox>
                <HBox fx:id="hBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="200.0" prefHeight="513.0" prefWidth="1087.0" VBox.vgrow="ALWAYS">
                    <children>
                        <ListView fx:id="listMods" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="5.0"/>
    </padding>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<fx:root type="javafx.scene.control.Tab" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <content>
        <BorderPane>
            <center>
                <TextArea fx:id="textArea" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
            </center>
            <top>
                <MenuBar BorderPane.alignment="CENTER">
                    <menus>
                        <Menu  mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem fx:id="upload" mnemonicParsing="false" text="Upload Log" />
                                <MenuItem fx:id="clear" mnemonicParsing="false" text="Clear Log" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Minecraft">
                            <MenuItem fx:id="kill" mnemonicParsing="false" text="Kill Minecraft"/>
                        </Menu>

                    </menus>
                </MenuBar>
            </top>
        </BorderPane>
    </content>
</fx:root>
